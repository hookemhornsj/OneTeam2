<h1>Listing required_skills</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @required_skills.each do |required_skill| %>
      <tr>
        <td><%= required_skill.request_id %></td>
        <td><%= required_skill.skill_id %></td>
        <td><%= link_to 'Show', required_skill %></td>
        <td><%= link_to 'Edit', edit_required_skill_path(required_skill) %></td>
        <td><%= link_to 'Destroy', required_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Required skill', new_required_skill_path %>
