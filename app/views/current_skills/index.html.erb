<h1>Listing current_skills</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @current_skills.each do |current_skill| %>
      <tr>
        <td><%= current_skill.user_id %></td>
        <td><%= current_skill.skill_id %></td>
        <td><%= link_to 'Show', current_skill %></td>
        <td><%= link_to 'Edit', edit_current_skill_path(current_skill) %></td>
        <td><%= link_to 'Destroy', current_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Current skill', new_current_skill_path %>
